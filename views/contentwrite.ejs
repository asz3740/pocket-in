<!doctype html>
<html>
<head>
    <%include template/head%>
</head>
<body style="height: 0px; background-image:url('/img/board.jpg'); background-size: cover;">
    <div class="container" style="position: relative; top:50px; width: 1200px; height: 800px;">
        <table class="table table-bordered" style="width: 1200px; height: 800px; border: 0 transparent;">
            <colgroup>
                <col width="50">
            </colgroup>
            <thead>
                <tr>
                    <td colspan="2">글쓰기</td>
                </tr>
            </thead>
            <tbody>
                <form id="writeAction" action="/process/contentwrite" method="post">
                <tr style="height:10px;">
                    <th scope="col" style="width:3%;">제목</th>
                    <td class=""><input type="text" class="inputSubject" id="title" name="title" placeholder="제목" style="width:550px;"></td>
                </tr>
                <tr style="height:200px;">
                    <th style="width:10%;">내용</th>
                    <td>
                        <textarea class="textContents" id="contents" name="contents" required="" style="width: 1050px; height: 450px; background-position: right top; margin: 0px;" placeholder="글 내용"></textarea>
                    </td>
                </tr>
                </form>
            </tbody>
        </table>
        <div class="forbtn">
            <button class="btn btn-primary"><a onclick="submitContents('add');">SUBMIT</a></button>
            <button class="btn btn-warning"><a class="wbtn" id="cancel">CANCEL</a></button>
        </div>
    </div>
    
    <script src="/js/cancel.js"></script>
</body>
</html>